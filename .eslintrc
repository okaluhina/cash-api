{
  "extends": ["standard", "prettier", "prettier/standard"],
  "plugins": ["prettier", "standard"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "space-before-function-parent": 0,
    "new-cap": 0,
    "prettier/prettier": 2,
    "no-unused-vars": 0,
    "camelcase": 0
  }
}
